<?xml version="1.0" encoding="UTF-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/listview_background_shape">
    <stroke
        android:width="1dp"
        android:color="@android:color/darker_gray" />
    <padding
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp" />
    <gradient
        android:startColor="#AADDFF"
        android:endColor="#BBEEBB"
        android:type="linear"
        ></gradient>
    <corners android:radius="@dimen/spacing"/>
</shape>